<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.text.Font?>

<BorderPane xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <left>
      <Button mnemonicParsing="false" prefHeight="60.0" prefWidth="52.0" text="&lt;" BorderPane.alignment="CENTER">
         <font>
            <Font name="System Bold" size="24.0" />
         </font>
      </Button>
   </left>
   <right>
      <Button mnemonicParsing="false" prefHeight="60.0" prefWidth="52.0" text="&gt;" BorderPane.alignment="CENTER">
         <font>
            <Font name="System Bold" size="24.0" />
         </font>
      </Button>
   </right>
   <top>
      <Label fx:id="statisticsTitle" alignment="CENTER" prefHeight="60.0" prefWidth="212.0" text="stat" textAlignment="CENTER" BorderPane.alignment="CENTER">
         <font>
            <Font name="System Italic" size="20.0" />
         </font>
      </Label>
   </top>
   <center>
      <Label fx:id="statisticInfo" alignment="CENTER" prefHeight="81.0" prefWidth="88.0" text="x" textAlignment="CENTER" BorderPane.alignment="CENTER">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
   </center>
</BorderPane>
